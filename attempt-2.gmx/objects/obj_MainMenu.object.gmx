                select = 0;
            }
            if(global.sfx) sound_play(snd_Select);
        }
        if(gamepad_button_check_pressed(i,global.cJump[i])){
            if(select == 0){
                room_goto(rm_ChoosePlayers);
            } else if(select == 1){
                room_goto(rm_Options);
            } else if(select == 2){
                game_end();
            }
        }
    }
} else {
