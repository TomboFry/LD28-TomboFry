            <string>if(xpos &gt; 0.5){
    xpos = xpos / 1.1;
} else {
    xpos = 0;
}

if(global.music){
    text[1] = "Music: Yep";
    if(!sound_isplaying(mus_MainMenu)){
        sound_stop_all();
        sound_loop(mus_MainMenu);
    }
} else {
    text[1] = "Music: Nope";
    if(sound_isplaying(mus_MainMenu)){
        sound_stop(mus_MainMenu);
    }
}

if(global.sfx){
    text[2] = "SFX: Yep";
} else {
    text[2] = "SFX: Nope";
}

if(window_get_fullscreen()){
    text[0] = "Fullscreen: Yep";
} else {
    text[0] = "Fullscreen: Nope";
}

count = getControlNum();
for(i = 0; i &lt; count; i++){
    if(gamepad_button_check_pressed(i,global.cUp[i])){
        if(select &gt; 0){
            select--;
        } else {
            select = 4;
        }
        if(global.sfx) sound_play(snd_Select);
    }
    if(gamepad_button_check_pressed(i,global.cDown[i])){
        if(select &lt; 4){
            select++;
        } else {
            select = 0;
        }
        if(global.sfx) sound_play(snd_Select);
    }
    if(gamepad_button_check_pressed(i,global.cJump[i])){
        if(select == 0){
            window_set_fullscreen(!window_get_fullscreen());
        } else if(select == 1){
            global.music = !global.music;
        } else if(select == 2){
            global.sfx = !global.sfx;
        } else if(select == 3){
            global.lastplayerpressed = i;
            room_goto(rm_KeyConfig);
        } else if(select == 4){
            room_goto(rm_Menu);
        }
    }
}

background_x[0] = 240-((xpos)/3);
</string>
